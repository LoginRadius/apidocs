{
    "name": "Custom Object Check",
    "domain": "https://api.loginradius.com",
    "path": "/raas/v1/user/customObject/check",
    "method": "GET",
    "getparams": {
        "appkey*": "@apikey@",
        "appsecret*": "@secret@",
        "objectid*": "",
        "accountid*": ""
    },
    "headers": {
        "content-Type": "application/json"
    },
    "response": "json",
    "description": "This API is used to check the presence of a Custom Object for the specified account ID(UID).",
    "parameters": {
        "Get Parameters": [
            {
                "name": "apikey",
                "is_required": true,
                "description": "Login Radius API Key"
            },
            {
                "name": "appsecret",
                "is_required": true,
                "description": "Login Radius API Secret"
            },
            {
                "name": "objectid",
                "is_required": true,
                "description": "LoginRadius Custom Objectid"
            },
            {
                "name": "accountid",
                "is_required": true,
                "description": "User's accountid"
            }
        ]
    },
    "outputFormat": {
        "IsExists": true
    }
}