{
    "name": "Account Username Check Server",
    "domain": "https://api.loginradius.com",
    "path": "/raas/v1/user/checkusername",
    "method": "GET",
    "getparams": {
        "appkey*": "@apikey@",
        "appsecret*": "@secret@",
        "username*": ""
    },
    "postparams": "",
    "headers": {
        "content-Type": "application/x-www-form-urlencoded"
    },
    "response": "json",
    "description": "This api is use to check the username(server) exists or not on your site.",
    "parameters": {
        "Get Params": [
            {
                "name": "api_key",
                "is_required": true,
                "description": "LoginRadius API Key"
            },
            {
                "name": "api_secret",
                "is_required": true,
                "description": "LoginRadius API Secret"
            },
            {
                "name": "username",
                "is_required": true,
                "description": "username of user"
            }
        ]
    },

    "outputFormat": {
    "isExist": true
}
}