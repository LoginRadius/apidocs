{
    "name": "Validate Access Token",
    "domain": "https://api.loginradius.com",
    "path": "/api/v2/access_token/Validate",
    "method": "GET",
    "getparams": {
        "key*": "@apikey@",
        "secret*": "@secret@",
        "access_token*": ""
    },
    "postparams": "",
    "headers": {
        "content-Type": "application/x-www-form-urlencoded"
    },
    "response": "json",
    "description": "This API validates access_token, if valid then returns a response with its expiry otherwise error.",
    "parameters": {
        "Get Params": [{
            "name": "key",
            "is_required": true,
            "description": "LoginRadius API Key"
        }, {
            "name": "secret",
            "is_required": true,
            "description": "LoginRadius API Secret"
        }, {
            "name": "access_token",
            "is_required": true,
            "description": "Uniquely generated identifier key by LoginRadius that is activated after successful authentication."
        }]
    },
    "outputFormat": {
  "access_token": "a123qwe2-9**3-4002-833c-3****o58cf66",
