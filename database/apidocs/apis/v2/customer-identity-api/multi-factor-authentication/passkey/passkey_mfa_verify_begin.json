    "domain": "https://api.loginradius.com",
    "path": "/identity/v2/auth/login/2fa/passkey/begin",
    "method": "GET",
    "getparams": {
        "apikey*": "@apikey@",
        "secondfactorauthenticationtoken*":""
    },
    "postparams":"",
