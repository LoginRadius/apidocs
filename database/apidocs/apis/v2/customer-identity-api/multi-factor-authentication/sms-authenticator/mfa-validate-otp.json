  "domain": "https://api.loginradius.com",
  "path": "/identity/v2/auth/login/2FA/verification/otp",
  "method": "PUT",
  "getparams": {
    "apikey*": "@apikey@",
    "secondfactorauthenticationtoken*": "",
    "smstemplate2fa": "",
    "isvoiceotp" : ""
