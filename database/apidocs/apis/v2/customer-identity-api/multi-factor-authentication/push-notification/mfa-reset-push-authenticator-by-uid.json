{
  "name": "Reset MFA Push Authenticator by UID",
  "domain": "https://api.loginradius.com",
  "path": "/identity/v2/manage/account/2FA/authenticator/push/{uid}",
  "method": "DELETE",
  "getparams": {
    "apikey*": "@apikey@",
        "apisecret*": "@secret@"
  },
  "postparams": "",
  "headers": {
    "content-Type": "application/json"
  },
  "response": "json",
  "description": "This API is used to reset the Push Authenticator settings for an MFA-enabled user.",
  
  "parameters": {
    "Get Params": [
      {
        "name": "apikey",
        "is_required": true,
        "description": "LoginRadius API Key"
      },
      {
        "name": "apisecret",
        "is_required": true,
        "description": "LoginRadius API Secret"
      }
    ],
    "Template Params": [
        {
            "name": "uid",
            "is_required": true,
            "description": "UID, the unified identifier for each user account"
        }
    ]
  },
  "outputFormat": {
    
          "IsDeleted": true
   }
}
