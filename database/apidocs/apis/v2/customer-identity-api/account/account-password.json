{
    "name": "Account Password",
    "domain": "https://api.loginradius.com",
    "path": "/identity/v2/manage/account/{uid}/password",
    "method": "GET",
    "getparams": {
        "apikey*": "@apikey@",
        "apisecret*": "@secret@"
    },
    "postparams": "",
    "headers": {
        "content-Type": "application/x-www-form-urlencoded"
    },
    "response": "json",
    "description": "You can use this API to retrieve the hashed password of a specified account in Cloud Directory.",
    "parameters": {
        "Get Params": [
            {
                "name": "apikey",
                "is_required": true,
                "description": "LoginRadius API Key"
            },
            {
                "name": "apisecret",
                "is_required": true,
                "description": "LoginRadius API Secret"
            }
        ],
        "Template Params": [
            {
                "name": "uid",
                "is_required": true,
                "description": "UID, the unified identifier for each user account"
            }
        ]
    },
    "outputFormat": {
        "PasswordHash": "Rfc2898Deriv*********A1lgjyagUkYkPiBhDjMDA==**********DWkIQd0dXyW666c="
    }
}
