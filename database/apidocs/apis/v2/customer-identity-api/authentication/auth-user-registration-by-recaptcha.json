        "Get Parameters": [
            {
                "name": "apikey",
                "is_required": true,
                "description": "Login Radius API Key"
            },
            {
                "name": "verificationurl",
                "is_required": false,
                "description": "Email verification url"
            },
            {
                "name": "emailtemplate",
                "is_required": false,
                "description": "Name of the email template to be sent out"
            },
            {
                "name": "smstemplate",
                "is_required": false,
                "description": "Name of the SMS template to be sent out"
            },
            {
                "name": "welcomeemailtemplate",
                "is_required": false,
                "description": "Name of the email template to be sent out"
            },
            {
                "name": "options",
                "is_required": false,
                "description": "PreventVerificationEmail (Specifying this value prevents the verification email from being sent. Only applicable if you have the Mandatory or Optional Email Verification Flow)"
            },
            {
                "name": "isvoiceotp",
                "is_required": false,
                "description": "Boolean, pass true if you wish to trigger voice OTP"
            }
        ],
        "Post Parameters": [
            {
                "name": "Email",
                "is_required": true,
                "description": "<b>Array</b> of <b>Email Objects</b>.<br> <b>Email Object</b> consist 2 properties:<br><b>'Value'</b>: The email string.<br><b>'Type'</b>: A string describing the email.<br> We suggest using 'Primary' as the <b>'Type'</b> on the first email registration.<br> See the <b>Body</b> tab below to see an example"
            },
            {
                "name": "Password",
                "is_required": true,
                "description": "Password for the email"
            },
            {
                "name": "PIN",
                "is_required": false,
                "description": "New desired PIN"
            },
            {
                "name": "Skipped",
                "is_required": false,
                "description": "Pass true, if user has skipped PIN popup otherwise pass false/null"
            },
            {
                "name": "Event",
                "is_required": false,
                "description": "Event name on which consent form should be shown"
            },
            {
                "name": "IsCustom",
                "is_required": false,
                "description": "Pass true, if the custom event is created by user, that is different from 3 LoginRadius events (Registration, FirstLogin, Login)"
            },
            {
                "name": "ConsentOptionId",
                "is_required": false,
                "description": "Unique identifier to identify a particular consent option. User can retreive this identifier from user profile"
            },   
            {
                "name": "IsAccepted",
                "is_required": false,
                "description": "Pass true, if consent is acccepted by user"
            },
            {
                "name": "g-recaptcha-response",
                "is_required": false,
                "description": "Pass the acknowledgement received by Google in Google recaptcha authorisation process."
            },
            {
                "name": "recaptcha_response_field",
                "is_required":false,
                "description": "Pass the V1 recaptcha field (optional in case of V2 recaptcha)"
            },
            {
                "name": "recaptcha_challenge_field",
                "is_required": false,
                "description": "Pass the V1 recaptcha field (optional in case of V2 recaptcha)"
            },
            {
                "name": "qq_captcha_ticket",
                "is_required": false,
                "description": "If <a href='/docs/api/v2/admin-console/platform-security/auth-security-configuration#bruteforcelockout1'>CAPTCHA lockout</a> is enabled with QQ, and the customer is locked, you must provide the QQ Captcha ticket."
            },
            {
                "name": "qq_captcha_randstr",
                "is_required": false,
                "description": "If <a href='/docs/api/v2/admin-console/platform-security/auth-security-configuration#bruteforcelockout1'>CAPTCHA lockout</a> is enabled with QQ, and the customer is locked, pass the value of the cusotmer's random string retrieved from the QQ captcha."
            },
            {
                "name": "Note",
                "is_required": false,
                "description": "Either you need to pass 'g-recaptcha-response' or 'qq_captcha_ticket' and 'qq_captcha_randstr' as per your configurations.<br>  <b>PIN</b> and <b>Skipped</b> fields are required only if <b>Pin Authentication flow</b> is enabled. <br> <b>Event</b>, <b>IsCustom</b>, <b>ConsentOptionId</b> and <b>IsAccepted</b> feilds are required only if <b>Consent Management feature</b> is enabled."
              }
        ]
    },
