{
    "name": "Phone Number Availability",
    "domain":"https://api.loginradius.com",
    "path": "/identity/v2/auth/phone",
    "method": "GET",
    "getparams": {
        "apikey*": "@apikey@",
        "phone*": ""
    },
    "postparams": "",
    "headers": {
        "content-Type": "application/x-www-form-urlencoded"
    },
    "response": "json",
    "description": "This API is used to check the Phone Number exists or not on your site.",
    "parameters": {
        "Get Params": [
            {
                "name": "apikey",
                "is_required": true,
                "description": "LoginRadius API Key"
            },
            {
                "name": "phone",
                "is_required": true,
                "description": "The Registered Phone Number"
            }
        ]
    },
    "outputFormat":{
    "IsExist": true
    }

}
