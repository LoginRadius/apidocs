{
    "name": "Auth Access Token Info",
    "domain": "https://api.loginradius.com",
    "path": "/identity/v2/auth/access_token",
    "method": "GET",
    "getparams": {
        "apikey*": "@apikey@"
    },
    "postparams": "",
    "headers": {
        "content-Type": "application/x-www-form-urlencoded",
	    "Authorization":"Bearer "
    },
    "response": "json",
    "description": "This API is used to obtain information on the provided access_token.",
    "parameters": {
        "Get Parameters": [{
                "name": "apikey",
                "is_required": true,
                "description": "LoginRadius API Key"
            }],
        "Headers Parameters": [{
                    "name": "Authorization",
                    "is_required": true,
                    "description": "Bearer &lt;ACCESS_TOKEN&gt; (customer's access token)"
                }]
    },
   "outputFormat": {  
   "provider":"facebook",
   "access_token":"********-****-****-****-************",
   "isrememberme":true
}}
