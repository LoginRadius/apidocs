{
  "name": "High Concurrency API Stack Check Username Availability",
  "domain": "https://api.loginradius.com",
  "path": "/hpa/v1/username/check",
  "method": "POST",
  "getparams": {
    "apikey*": "@apikey@"
  },
  "postparams": {
    "username*": ""
  },
  "headers": {
    "content-Type": "application/x-www-form-urlencoded"
  },
  "response": "json",
  "description": "This API is used to check for the availability of a username.",
  "parameters": {
    "Get Params": [{
      "name": "apikey",
      "is_required": true,
      "description": "LoginRadius API Key"
    }],
    "Post Params": [{
        "name": "username",
        "is_required": true,
        "description": "The username to be checked for availability"
      }
    ]

  },
  "outputFormat": {
    "Exists": true
  }
}
