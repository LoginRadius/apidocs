{
    "name": "Roles by UID",
    "domain":"https://api.loginradius.com",
    "path": "/identity/v2/manage/account/{uid}/role",
    "method": "GET",
    "getparams": {
        "apikey*": "@apikey@",
        "apisecret*": "@secret@"
    },
    "postparams": "",
    "headers": {
        "content-Type": "application/x-www-form-urlencoded"
    },
    "response": "json",
    "description": "API is used to retrieve all the assigned roles of a particular User.",
    "parameters": {
        "Get Params": [
          {
            "name": "apikey",
            "is_required": true,
            "description": "Login Radius API Key"
          },
          {
            "name": "apisecret",
            "is_required": true,
            "description": "LoginRadius API Secret"
          }
        ],
        "Template Param": [
          {
            "name": "uid",
            "is_required": true,
            "description": "User's UID"
          }
        ]
    },
  "outputFormat": {
    "Roles": [
      "Editor"
    ]
  }
}
