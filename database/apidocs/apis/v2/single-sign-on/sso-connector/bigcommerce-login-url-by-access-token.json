{
  "name": "Bigcommerce Login URL by Access Token",
  "domain": "https://cloud-api.loginradius.com",
  "path": "/sso/bigcommerce/api/token",
  "method": "GET",
  "getparams": {
    "apikey*": "@apikey@",
    "store*": "",
    "password":"",
    "return_url": "",
    "callback": ""
  },
  "headers": {
    "content-Type": "application/x-www-form-urlencoded",
    "Authorization":"Bearer "
  },
  "response": "json",
  "description": "This API is used to get the Bigcommerce Login URL by access token.",
  "parameters": {
    "Get Params": [
      {
        "name": "apikey",
        "is_required": true,
        "description": "LoginRadius API Key"
      },
      {
        "name": "store",
        "is_required": true,
        "description": "Store Name"
      },
      {
        "name": "password",
        "is_required": false,
        "description": "Bigcommerce password for new user"
      },
      {
        "name": "return_url",
        "is_required": false,
        "description": "After login page URL"
      },
      {
        "name": "callback",
        "is_required": false,
        "description": "JSONP callback function name"
      }

    ],
    "Headers Parameters": [{
      "name": "Authorization",
      "is_required": true,
      "description": "Bearer &lt;ACCESS_TOKEN&gt; (User's access token)"
